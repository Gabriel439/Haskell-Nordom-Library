    \(a       : *                                    )
->  \(xs      : ../Vector a                          )
->  \(Maybe   : *                                    )
->  \(Nothing : Maybe                                )
->  \(Just    : forall (_Just : ../Vector a) -> Maybe)
->  \(Nothing : Maybe                                )
->  ./null a xs
    Maybe
    Nothing
    (Just (./drop 1 a xs))
