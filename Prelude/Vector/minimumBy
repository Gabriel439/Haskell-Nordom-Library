    \(a       : *                     )
->  \(cmp     : a -> a -> ../Ordering )
->  \(xs      : ../Vector a           )
->  ../Vector/foldr
    a
    xs
    (../Maybe a)
    (   \(x : a         )
    ->  \(m : ../Maybe a)
    ->  m
        (../Maybe a)
        (\(y : a) ->
            cmp
            x
